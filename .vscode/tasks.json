{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "lint",
			"type": "shell",
			"command": "npm run lint",
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "build",
			"type": "shell",
			"command": "npm run build",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		},
		{
			"label": "build-perf",
			"type": "shell",
			"command": "pnpm build",
			"group": "build"
		},
		{
			"label": "build-stretch",
			"type": "shell",
			"command": "pnpm build",
			"group": "build"
		},
		{
			"label": "build-stretch",
			"type": "shell",
			"command": "pnpm build",
			"args": [],
			"isBackground": false,
			"problemMatcher": [
				"$tsc"
			],
			"group": "build"
		}
	]
}